{
  "tesseractTrainingDataPath": "../tesseractTrainingData/",
  "server": {
    "maxFileSize": 1000,
    "maxNumberOfFiles": 1,
    "tmpDirectoryPath": "./tmp/img/",
    "supportedMimeTypes": [
      "image/jpeg",
      "image/png",
      "image/jpg",
      "image/tiff"
    ],
    "errors": {
      "noFileUploaded": {
        "code": 204,
        "text": "No file uploaded."
      },
      "noTextFound": {
        "code": 206,
        "text": "No text found in the given image."
      },
      "wrongMimeType": {
        "code": 406,
        "text": "Wrong file type, only image or pdf allowed."
      },
      "InternalError": {
        "code": 500,
        "text": "Internal Server Error"
      },
      "WrongParamName": {
        "code": 412,
        "text": "File found but sent with wrong param name. Please use 'image'."
      }
    }
  },
  "application": {
  }
}